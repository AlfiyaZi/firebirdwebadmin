#!/bin/sh
# This maintainer script was generated by yada

set -e

PACKAGE="ibwebadmin"
SOURCE="ibwebadmin"
VERSION="2.0-1ubuntu0"

if [ "$1" = "purge" ]; then
    for ext in '~' '%' .bak .dpkg-tmp .dpkg-new .dpkg-old .dpkg-dist; do
        rm -f "/etc/ibwebadmin/configuration.inc.php$ext"
    done
    rm -f "/etc/ibwebadmin/configuration.inc.php"
    if [ -x "`which ucf 2>/dev/null`" ]; then ucf --purge "/etc/ibwebadmin/configuration.inc.php"; fi
fi

exit 0
